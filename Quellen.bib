@book{Gerthsen.2015,
 author = {Meschede, Dieter},
 year = {2015},
 title = {Gerthsen Physik},
 keywords = {Atomphysik;Dynamik;Elektrodynamik;Elektromagnetismus;Festkörperphysik;Kerne und Elementarteilchen;Laserphysik;Mechanik;Optik;relativistische Physik;Statistische Physik;Strahlungsfelder;Wärme},
