@article{Borzsonyi.2008,
 author = {Börzsönyi, A. and Heiner, Z. and Kalashnikov, M. P. and Kovács, A. P. and Osvay, K.},
 year = {2008},
 title = {Dispersion measurement of inert gases and gas mixtures at 800 nm},
 pages = {4856--4863},
 pagination = {page},
